	   -webkit-margin-end: 0px;
}

a {
	-webkit-transition: all .300s;
	   -moz-transition: all .300s;
		 -o-transition: all .300s;
			transition: all .300s;
}

a, a:focus, a:hover, a:active {
  outline: 0;
  cursor:pointer;
}

p, pre, ul, ol, dl, dd, blockquote,
address, table, fieldset, form {
