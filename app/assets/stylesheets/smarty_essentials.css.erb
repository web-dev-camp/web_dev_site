	http://www.paulund.co.uk/creating-different-css3-box-shadows-effects
**************************************************************** **/
.box-shadow-1:after,
.box-shadow-1:before {
	top: 80%;
	left: 5px;
	width: 50%;
	z-index: -1;
	content: "";
	bottom: 15px;
	max-width: 300px;
	position: absolute;
}

/* Shadow 1 */
.box-shadow-1 {
	-webkit-box-shadow: 0 14px 6px -6px #666;
	   -moz-box-shadow: 0 14px 6px -6px #666;
			box-shadow: 0 14px 6px -6px #666;
}



/* Shadow 2 */
.box-shadow-2 {
	position: relative;
}
.box-shadow-2:before,
.box-shadow-2:after {
	z-index: -1;
	position: absolute;
	content: "";
	bottom: 15px;
	left: 10px;
	width: 50%;
	top: 80%;
	max-width:300px;
	background: #777;

	-webkit-box-shadow: 0 15px 10px #777;
	   -moz-box-shadow: 0 15px 10px #777;
